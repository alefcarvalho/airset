sleep 1

clear
top
echo "Would you like to Change the Wlan WIFI_MONITOR's MAC Address?"
		echo "                                       "
		echo -e "      "$red"["$yellow"1"$red"]"$transparent" Yes"
		echo -e "      "$red"["$yellow"2"$red"]"$transparent" No"


read c
case $c in
	1)
clear
top
mac_change_1

sleep 1

clear
;;
	2)
clear
;;
	*)Invalid Option
;;
esac


