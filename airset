{
echo "Setting the MAC Address on $WIFI_MONITOR1"
ifconfig $WIFI_MONITOR1 down
sleep 3
macchanger $WIFI_MONITOR1 -m 02:22:88:29:EC:6F
sleep 3
ifconfig $WIFI_MONITOR1 up
echo "MAC Changed"
}

